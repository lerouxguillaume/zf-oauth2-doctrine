{
    "name": "samsonasik/zf-oauth2-doctrine",
    "description": "Doctrine OAuth2 Server Adapter for Apigility",
    "license": "MIT",
    "keywords": [
        "oauth2",
        "doctrine",
        "laminas-api-tools"
    ],
    "require": {
        "php": "^7.4 | ~8.0",
        "laminas-api-tools/api-tools-oauth2": "^1.4",
        "doctrine/orm": "^2.5.11",
        "doctrine/doctrine-module": "^4.0 || ^5.0",
        "doctrine/doctrine-orm-module": "^4.0 || ^5.0",
        "laminas-api-tools/api-tools-mvc-auth": "^1.4.3",
        "laminas/laminas-mvc": "^3.0",
        "laminas/laminas-config": "^3.0",
        "laminas/laminas-modulemanager": "^2.0",
        "laminas/laminas-servicemanager": "^3.0",
        "laminas/laminas-i18n": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "laminas/laminas-form": "^3.1",
        "laminas/laminas-loader": "^2.4",
        "laminas/laminas-log": "^2.9",
        "laminas/laminas-serializer": "^2.7",
        "laminas/laminas-test": "^4.0",
        "laminas/laminas-authentication": "^2.5",
        "laminas/laminas-eventmanager": "^3.2",
        "mockery/mockery": "^1.0",
        "php-coveralls/php-coveralls": "^2.5",
        "api-skeletons/coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ZF\\OAuth2\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\OAuth2\\Doctrine\\": "test/"
        }
    },
    "extra": {
        "laminas": {
            "component": "ZF\\OAuth2\\Doctrine",
            "config-provider": "ZF\\OAuth2\\Doctrine\\ConfigProvider"
        }
    },
    "suggest": {
        "api-skeletons/zf-oauth2-doctrine-permissions-acl": "Access Control Lists for zf-oauth2-doctrine",
        "bushbaby/zf-oauth2-doctrine-mutatetablenames": "Allows configuration of the table names that this module uses"
    }
}
